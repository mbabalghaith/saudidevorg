car1 = {'name':'Ford','Color':'red','Year':'2019'}

car2 = {'name':'Mazda','Color':'silver','year':'2018'}

car3 = {'name':'toyota','color':'blue','year':'2017'}

allcars = {'car1': car1 , 'car2' : car2 , 'car3' : car3 }

print (allcars)

dicfun = dict(brand = 'Ford' , color = 'yellow' , year = '1998')

print (dicfun)