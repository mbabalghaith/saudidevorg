thelist = {'orange','apple','cherry','banana','limon'}

thelist.remove('apple')
print (thelist)

thelist.pop()

print (thelist)
print ('-------------------')

list2 = set(('mohammed','ahmed','ali','khalid','nasser'))

print (list2)

list2.clear()

print (list2)

list2.add('Mohammed')
print (list2)

print (len(list2))