tuple1 = ('mohammed' , 'ali' , 'khalid' ,'nasser')
tuple2 = (3,)
print(tuple2)
print (tuple1)
print('-----------------')

for i in tuple1:
    print (i)

print('--------------------')



print (tuple1[0:2])
print ('------------------------------')

if 'ali' in tuple1:
    print ('Yes ali is there')

else:
    print ('No')

repeat = ('python , ') * 4
print (tuple1 + tuple2)

print (repeat)

print ('--------------')

print (len(repeat))

print (len(tuple1))

thistuple = tuple(('java' , 'python ' , 'php' , 'C++'))

print (thistuple)

cnt = thistuple.count('C++')

print ('The count of php is: ', cnt)

print ('--------------------')

mm = ['java' , 'php' , 'python ' , 'C#']

nn = tuple(mm)

print (mm)
print (nn)

cnt1  = mm.count('python')
cnt2 = nn.count('python')
print ('---------------')
print (cnt1)
print (cnt2)

print (tuple1.index('khalid'))
print (tuple1.count('khalid'))

print ('count return 0 or 1, index return the index of element, index just work with tuples')