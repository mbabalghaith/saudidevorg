set = {1,3,5,7,8}
print (set)

print ('First EX: Add 4,8,12')
set.update([4,8,12])
print (set)

set2 = {'name':'pigeon','type':'bird','skin cover':'feathers'}

print ('------------------------')

print (set2)

print ('Second EX: A)--> print value of type:')

for x ,y in set2.items():
    if x == 'type':
        print (y)
        break
print ('---------------')
print ('Second EX: B)--> Change value of skin cover')

set2['skin cover']='leather'

print (set2)